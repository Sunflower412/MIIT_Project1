<Window x:Class="wefrwefw.OpenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wefrwefw"
        mc:Ignorable="d"
        Title="OpenWindow" Height="450" Width="800" Topmost="True">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/АСУССлоготип.7ab51a28-028d-4de2-9ba2-3d0c60dcce03"/>
        </Grid.Background>
        <Button x:Name="ButtonStart" Content="Начать курс" Margin="0,0,0,112" Click="ButtonStart_Click" FontSize="22" Background="White" Foreground="#FFB3096E" BorderBrush="Black" BorderThickness="2,1,1,1" Padding="2,1,1,1" UseLayoutRounding="False" HorizontalAlignment="Center" Width="280" Height="36" VerticalAlignment="Bottom"/>
        <Button x:Name="ButtonExit" Content="Выйти" Margin="0,0,0,71" Background="White" Foreground="#FFB3096E" FontSize="22" Click="ButtonExit_Click" HorizontalAlignment="Center" Width="280" Height="36" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
